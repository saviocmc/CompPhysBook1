@article{patriarca2004,
	Abstract = {We review a simple model of closed economy, where the economic agents
	make money transactions and a saving criterion is present. We observe
	the Gibbs distribution for zero saving propensity, and non-Gibbs
	distributions otherwise. While the exact solution in the case of
	zero saving propensity is already known to be given by the Gibbs
