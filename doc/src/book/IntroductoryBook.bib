@article{ball2002,
	Abstract = {A method is developed for calculating the recurrence coefficients
	for half-range generalized Hermite polynomials. These are orthogonal
	polynomials with measure $x^{\gamma}e^{-x^{2}}$on the interval ($0,\infty$).
	The recurrence coefficients can then be used to generate the weights
	and nodes of the related Gaussian quadratures. These quadratures
	allow efficient high accuracy evaluation of many Gaussian integrals
	encountered in probability functions, statistical mechanics, and
	quantum mechanics. The number of steps for an accurate numerical
	calculation of the recurrence coefficients is proportional to N,
	the number of coefficients obtained. Extended precision arithmetic
	is not needed in these calculations.},
	Author = {James S. Ball},
