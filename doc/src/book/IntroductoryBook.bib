	Bdsk-Url-1 = {http://link.aip.org/link/?JCP/76/1064/1},
	Bdsk-Url-2 = {http://dx.doi.org/10.1063/1.443098}}

@article{schmidt1982a,
	Abstract = {The potential energy surface of the LiH molecule is calculated using
