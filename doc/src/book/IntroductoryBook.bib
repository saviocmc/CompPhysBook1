@article{papenbrock:054305,
	Abstract = {We investigate analytically and numerically a random-matrix model
	for m fermions occupying [script-l]1 single-particle states with
	positive parity and [script-l]2 single-particle states with negative
	parity and interacting through random two-body forces that conserve
	parity. The single-particle states are completely degenerate and
	carry no further quantum numbers. We compare spectra of many-body
	states with positive and with negative parity. We show that in the
	dilute limit defined by m,[script-l]1,2-->[infinity] and m/[script-l]1,2-->0,
	ground states with positive and negative parity occur with equal
	probability. Differences in the ground-state probabilities are, thus,
	a finite-size effect and are mainly due to different dimensions of
	the Hilbert spaces of either parity.},
	Author = {T. Papenbrock and H. A. Weidenm\"{u}ller},
	Doi = {10.1103/PhysRevC.78.054305},
	Eid = {054305},
