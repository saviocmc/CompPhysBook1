@article{PhysRevC.83.014606,
	Abstract = {Single-neutron-transfer measurements using (p,d) reactions have been
	performed at 33 MeV per nucleon with proton-rich 34Ar and neutron-rich
	46Ar beams in inverse kinematics. The extracted spectroscopic factors
	are compared to the large-basis shell-model calculations. Relatively
	weak quenching of the spectroscopic factors is observed between 34Ar
	and 46Ar. The experimental results suggest that neutron correlations
	have a weak dependence on the asymmetry of the nucleus over this
	isotopic region. The present results are consistent with the systematics
	established from extensive studies of spectroscopic factors and dispersive
	optical-model analyses of 40-49Ca isotopes. They are, however, inconsistent
	with the trends obtained in knockout-reaction measurements.},
	Author = {Lee, Jenny and Tsang, M.B. and Bazin, D. and Coupland, D. and Henzl, V. and Henzlova, D. and Kilburn, M. and Lynch, W. G. and Rogers, A. M. and Sanetullaev, A. and Sun, Z. Y. and Youngs, M. and Charity, R. J. and Sobotka, L. G. and Famiano, M. and Hudan, S. and Shapira, D. and O'Malley, P. and Peters, W. A. and Chae, K. Y. and Schmitt, K.},
