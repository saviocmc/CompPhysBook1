@article{Jungclaus2011,
	Abstract = {Precise measurements of the lifetimes of the first excited 2+ states
	in the stable even-A Sn isotopes 112--124Sn have been performed using
	the Doppler shift attenuation technique. For the isotopes 112Sn,
	114Sn and 116Sn the E2 transition strengths deduced from the measured
	lifetimes are in disagreement with the previously reported values
