@article{tripathi2008,
	Abstract = {The beta decay of 33Mg (N=21) presented in this Letter reveals intruder
	configurations in both the parent and the daughter nucleus. The lowest
	excited states in the N=20 daughter nucleus, 33Al, are found to have
	nearly 2p-2h intruder configuration, thus extending the ?island of
