@article{hagen2007b,
	Abstract = {We present ab initio calculations for 3H, 4He, 16O, and 40Ca based
	on two-nucleon low-momentum interactions Vlow k within coupled-cluster
	theory. For 3H and 4He, our results are within 70 and 10 keV of the
	corresponding Faddeev and Faddeev-Yakubovsky energies. We study in
